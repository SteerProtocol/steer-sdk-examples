{
  "name": "sdk-examples",
  "version": "1.0.0",
  "description": "TypeScript SDK examples and patterns",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "example:deposit": "ts-node -r tsconfig-paths/register src/smart-pool-deposit/index.ts",
    "example:withdrawal": "tsc && node dist/smart-pool-withdrawal/index.js examples",
    "example:vaults": "ts-node -r tsconfig-paths/register src/steer-pool-get-vaults/index.ts",
    "example:blackhole-rewards": "ts-node -r tsconfig-paths/register src/steer-pool-pending-rewards/index.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "sdk",
    "examples",
    "patterns"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@steerprotocol/sdk": "1.22.0-test-blackhole-support.2",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v3-sdk": "^3.25.2",
    "axios": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0"
  }
}
